<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>guideGet</servlet-name>
		<servlet-class>guidepage.action.Get</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>guideAdd</servlet-name>
		<servlet-class>guidepage.action.Add</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>tokenGet</servlet-name>
		<servlet-class>common.token.Get</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>guideUpdate</servlet-name>
		<servlet-class>guidepage.action.Update</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>guideDelete</servlet-name>
		<servlet-class>guidepage.action.Delete</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>guideDelete</servlet-name>
		<url-pattern>/guidePage/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>guideUpdate</servlet-name>
		<url-pattern>/guidePage/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>tokenGet</servlet-name>
		<url-pattern>/token/get</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>guideGet</servlet-name>
		<url-pattern>/guidePage/get</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>guideAdd</servlet-name>
		<url-pattern>/guidePage/add</url-pattern>
	</servlet-mapping>
</web-app>